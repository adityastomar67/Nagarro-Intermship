<%- include('../partials/header.ejs') %>

  <div class="container">
    <div class="row">
      <div class="col-lg-3"></div>
      <div class="col-lg-6 mt-4">
            <h1>Login Page</h1>
        <form action="/register" method="POST">
          <div class="mb-3">
            <input type="text" class="form-control" name="" id="" value="" placeholder="firstName">
          </div>
          <div class="mb-3">
            <input type="text" class="form-control" name="" id="" value="" placeholder="lastName">
          </div>
          <div class="mb-3">
            <input type="email" class="form-control" name="" id="" value="" placeholder="email">
          </div>
          <div class="mb-3">
            <input type="text" class="form-control" name="" id="" value="" placeholder="username">
          </div>
          <div class="mb-3">
            <input type="password" class="form-control" name="" id="" value="" placeholder="password">
          </div>
          <button class="btn btn-primary" type="submit">Submit</button>
        </form>
      </div>
      <div class="col-lg-3"></div>
    </div>
  </div>

  <%- include('../partials/footer') %>
